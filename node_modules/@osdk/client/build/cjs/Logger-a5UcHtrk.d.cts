interface Logger {
    trace: LogFn;
    debug: LogFn;
    fatal: LogFn;
    error: LogFn;
    warn: LogFn;
    info: LogFn;
    isLevelEnabled(level: string): boolean;
    child(bindings: Record<string, any>, options?: {
        level?: string;
        msgPrefix?: string;
    }): Logger;
}
interface LogFn {
    (obj: unknown, msg?: string, ...args: any[]): void;
    (msg: string, ...args: any[]): void;
}

export type { Logger as L, LogFn as a };
